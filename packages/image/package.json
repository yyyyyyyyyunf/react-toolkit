{
	"name": "@fly4react/image",
	"version": "1.7.0",
	"description": "Image optimization and lazy loading utilities for React",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rslib build",
		"dev": "rslib build --watch",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage",
		"check": "biome lint src/ --skip=a11y",
		"format": "biome format src/ --write",
		"lint": "biome lint src/ --skip=a11y",
		"lint:fix": "biome lint src/ --write --skip=a11y"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.5.3",
		"@rsbuild/plugin-react": "^1.3.5",
		"@rslib/core": "^0.14.0",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^12.1.5",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.6.1",
		"@types/react": "^16.8.0",
		"@vitest/ui": "^3.2.4",
		"jsdom": "^26.1.0",
		"react": "^16.8.0",
		"react-dom": "^16.8.0",
		"typescript": "^5.9.2",
		"vitest": "^3.2.4",
		"@fly4react/observer": "^1.5.2",
		"@fly4react/memo": "^1.0.10"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0",
		"@fly4react/observer": "^1.5.2",
		"@fly4react/memo": "^1.0.10"
	},
	"keywords": [
