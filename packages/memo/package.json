{
  "name": "@react-toolkit/memo",
  "version": "1.0.0",
  "description": "一个高级的 React 记忆化组件工具，提供灵活的 props 比较策略",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "memo",
    "performance",
    "optimization",
    "react-toolkit"
  ],
  "author": "yyyyyyyyyunf",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yyyyyyyyyunf/react-toolkit.git"
  },
  "bugs": {
    "url": "https://github.com/yyyyyyyyyunf/react-toolkit/issues"
  },
  "homepage": "https://github.com/yyyyyyyyyunf/react-toolkit#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.4.0",
    "@rsbuild/plugin-react": "^0.4.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm check && pnpm build",
    "prepack": "pnpm build",
    "prepare": "[ -d .git ] && husky || true"
  }
}
